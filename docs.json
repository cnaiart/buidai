{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "必定AI",
  "colors": {
    "primary": "#2563EB",
    "light": "#3B82F6",
    "dark": "#1D4ED8"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "指南",
        "groups": [
          {
            "group": "入门指南",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "安装与体验",
            "pages": [
              "introduction/docker",
              "introduction/bt",
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "编写内容",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "开发文档",
        "groups": [
          {
            "group": "开发指南",
            "pages": [
              "develop/Preposition",
              "develop/develop",
              "develop/directory",
              "develop/config"
            ]
          },
          {
            "group": "PC客户端",
            "pages": [
              "develop/pcdevelop",
              "develop/build"
            ]
          }
        ]
      },
      {
        "tab": "API 参考",
        "groups": [
          {
            "group": "API 文档",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "端点示例",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "必定官网",
          "href": "https://buidai.com/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "ApiKey",
          "href": "https://api.buidai.com",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "官网",
        "href": "https://buidai.com/"
      }
    ],
    "primary": {
      "type": "button",
      "label": "APIKey",
      "href": "https://api.buidai.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
